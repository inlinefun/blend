{
	"schemaVersion": 1,
	"id": "blend",
	"version": "${version}",
	"name": "Blend",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"inlinefun"
	],
	"contact": {
		"sources": "https://github.com/inlinefun/blend"
	},
	"license": "CC0-1.0",
	"icon": "assets/blend/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			{
				"value": "blend.Initializer",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"blend.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${fabric_loader}",
		"minecraft": "~${minecraft}",
		"java": ">=21",
		"fabric-language-kotlin": ">=${fabric_kotlin}"
	},
	"suggests": {
		"another-mod": "*"
	}
}